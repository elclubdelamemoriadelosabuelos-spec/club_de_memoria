{
  "headers": [
    {
      "source": "/(.*\\.(mp4|webm|mov|avi))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/(.*\\.webm)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/webm"
        }
      ]
    },
    {
      "source": "/(.*\\.(jpg|jpeg|png|webp|gif|svg))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
